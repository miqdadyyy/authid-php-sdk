{
    "name": "stia/authid-php",
    "description": "SDK / Plugin to handle Authenticator for Auth ID",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^11.0",
        "pragmarx/google2fa": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Stia\\AuthidPhp\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Miqdad Farcha",
            "email": "miqdad.farcha@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stia\\AuthidPhp\\AuthIDServiceProvider"
            ]
        }
    }
}
